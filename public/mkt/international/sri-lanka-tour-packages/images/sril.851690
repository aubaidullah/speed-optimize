<script>
    $( window ).load(function() {
        setupNavigation();
    });

    $( window ).resize(function() {
        setupNavigation();
    });

    function setupNavigation(){
        resizeSwiper();
        let nav = getNavItems();
        nav.left.style.display = 'none';
        nav.right.style.display = 'none';
        if(isNavSwipeable() && ($(window).width() > 991) ){
            nav.left.style.display = 'inline-block';
            nav.right.style.display = 'inline-block';
        }
    }

    function isNavSwipeable(){
        let nav = getNavItems();
        let totalWidth = 0;
        for (let i = 0; i < nav.children.length; i++) {
            if(nav.children[i]){
                totalWidth += nav.children[i].offsetWidth;
            }
        }
        return (nav.parent.clientWidth < totalWidth);
    }

    function resizeSwiper(){
        let nav = getNavItems();
        let totalWidth = 0;
        for (let i = 0; i < nav.children.length; i++) {
            if(nav.children[i]){
                totalWidth += nav.children[i].offsetWidth;
            }
        }
        nav.swiper.style.width = totalWidth + 30 +  "px";
    }

    function scrollNav(direction){
        let nav = getNavItems();
        if(direction == 'right'){
            nav.parent.scrollLeft += 250;
        }else if(direction == 'left'){
            nav.parent.scrollLeft -= 250;
        }
    }

    function getNavItems(){
        let swiper = document.getElementById('swipey-nav');
        let left = document.getElementById('left-nav-control');
        let right = document.getElementById('right-nav-control');
        if ($(window).width() < 991) {
            swiper = document.getElementById('sm-swipey-nav');
            left = document.getElementById('sm-left-nav-control');
            right = document.getElementById('sm-right-nav-control');
        }
        return {
            "swiper": swiper,
            "parent": swiper.parentNode,
            "children": swiper.children,
            "left": left,
            "right": right,
        };
    }
</script>
